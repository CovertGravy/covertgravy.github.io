{
  "name": "portfolio-v1",
  "version": "1.0.0",
  "description": "Portfolio version 1",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/*.html src/index.html",
    "predeploy": "rm -rf dist && parcel build src/*.html src/index.html",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "portfolio",
    "developer",
    "looper",
    "parcel",
    "tailwindcss"
  ],
  "author": "Ashish Shaji",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "autoprefixer": "^9.8.6",
    "gh-pages": "^3.1.0",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "locomotive-scroll": "^3.6.1",
    "normalize.css": "^8.0.1",
    "parcel": "^2.2.1",
    "postcss": "^8.4.6",
    "topbar": "^0.1.4",
    "vanilla-tilt": "^1.7.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  }
}
